  100 N=9
  110 DIM A(N)
  120 DATA 0.237, 0.445, 0.698, 0.914, 1.0, 0.914, 0.698, 0.445, 0.237
  130 RESTORE
  140 FOR I=0 TO N-1
  150 READ A(I)
  160 NEXT I
  170 FOR I=0 TO N-1
  180 SUM = SUM + A(I)
  190 NEXT I
  200 AVE = SUM/N
  210 FOR I=0 TO N-1
  220 SIGMA = SIGMA + (A(I)-AVE)*(A(I)-AVE)
  230 NEXT I
  240 SIGMA = SIGMA/(N-1)
  250 FOR I=0 TO N-1
  260 KURT = KURT + ( (A(I)-AVE)*(A(I)-AVE)*(A(I)-AVE)*(A(I)-AVE) ) / (SIGMA*SIGMA)
  270 NEXT I
  280 KURT = KURT * ( (N*(N+1)) / ((N-1)*(N-2)*(N-3)) )
  290 KURT = KURT - ( 3*(N-1)*(N-1) ) / ( (N-2)*(N-3) )
  300 PRINT SUM, AVE, SIGMA, KURT
  310 END
